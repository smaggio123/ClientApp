<Window xmlns="https://github.com/avaloniaui"
		xmlns:views="clr-namespace:ClientApp.Views"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:ClientApp.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
		xmlns:local= "clr-namespace:ClientApp"
        x:Class="ClientApp.Views.MainWindow"
		x:DataType="vm:MainWindowViewModel"
        Icon="/Assets/avalonia-logo.ico"
        Title="ClientApp">

    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>
	
	<TextBlock Text="{CompiledBinding Greeting}" HorizontalAlignment="Center" FontSize="20" Margin="0,50,0,0"/>
	

</Window>
